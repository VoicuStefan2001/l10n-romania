        <field name="groups_id" eval="[(4, ref('base.group_no_one'))]" />
        <field name="state">code</field>
        <field name="code">records.correction_create_svl()</field>
    </record>
    <record id="action_stock_move_correction_recreate_valuation" model="ir.actions.server">
