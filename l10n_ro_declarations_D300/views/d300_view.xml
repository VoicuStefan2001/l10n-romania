<odoo>

    <record id="view_d300_tree" model="ir.ui.view">
        <field name="name">l10n_ro.d300.tree</field>
        <field name="model">l10n_ro.d300</field>
        <field name="type">tree</field>
        <field name="arch" type="xml">
            <tree>
                <field name="partner_type"/>
                <field name="partner_id"/>
                <field name="net" sum="net"/>
                <field name="tax" sum="tax"/>
                <field name="company_id" options="{'no_create': True}" groups="base.group_multi_company"/>
            </tree>
        </field>
    </record>


    <record id="action_D300" model="ir.actions.act_window">
        <field name="name">D300</field>
        <field name="res_model">l10n_ro.d300</field>
        <field name="view_type">form</field>
        <field name="view_mode">tree,pivot</field>
    </record>

